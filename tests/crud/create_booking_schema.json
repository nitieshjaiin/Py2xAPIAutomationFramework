{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "title": "Root Schema",
    "type": "object",
    "default": {},
    "required": [
        "bookingid",
        "booking"
    ],
    "additionalProperties": false,
    "properties": {
        "bookingid": {
            "title": "The bookingid Schema",
            "type": "integer",
            "default": 0
        },
        "booking": {
            "title": "The booking Schema",
            "type": "object",
            "default": {},
            "required": [
                "firstname",
                "lastname",
                "totalprice",
                "depositpaid",
                "bookingdates",
                "additionalneeds"
            ],
            "additionalProperties": true,
            "properties": {
                "firstname": {
                    "title": "The firstname Schema",
                    "type": "string",
                    "default": ""
                },
                "lastname": {
                    "title": "The lastname Schema",
                    "type": "string",
                    "default": ""
                },
                "totalprice": {
                    "title": "The totalprice Schema",
                    "type": "integer",
                    "default": 0
                },
                "depositpaid": {
                    "title": "The depositpaid Schema",
                    "type": "boolean",
                    "default": false
                },
                "bookingdates": {
                    "title": "The bookingdates Schema",
                    "type": "object",
                    "default": {},
                    "required": [
                        "checkin",
                        "checkout"
                    ],
                    "additionalProperties": true,
                    "properties": {
                        "checkin": {
                            "title": "The checkin Schema",
                            "type": "string",
                            "default": ""
                        },
                        "checkout": {
                            "title": "The checkout Schema",
                            "type": "string",
                            "default": ""
                        }
                    }
                },
                "additionalneeds": {
                    "title": "The additionalneeds Schema",
                    "type": "string",
                    "default": ""
                }
            }
        }
    }
}